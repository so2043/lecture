package 연습문제._07장;

import java.util.Arrays;

class C {
	
	void abc(int m) {
		m = 8;
	}
	
	void bcd(int[] n) {
		n[0] = 4;
		n[1] = 5;
		n[2] = 6;
	}
}


public class Unit07_04번 {
	public static void main(String[] args) {
		
		C c = new C();
		
		int m = 5;
		int[] n = {1,2,3};
		
		c.abc(m);
		c.bcd(n);
		
		
		System.out.println(m);
		System.out.println(Arrays.toString(n));
	}
}
