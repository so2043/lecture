package control;

public class DoWhileTest1 {
	public static void main(String[] args) {
		
		
		int i = 1;
		while (i <= 3) {
			System.out.println("sysone");
			i++;
		}
		System.out.println("종료");
		
		do {
			System.out.println("sysone");
			i++;
		} while {
			System.out.println("sysone");
		}
		
		
		
		System.out.println("종료");
	}
}
