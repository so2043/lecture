package _23_innerclass.sec01_instanceinnerclass.EX02_UseMemberOfOuterClass;


class A {
	public int a = 3;
	protected int b = 4;
	int c = 5;
	private int d = 6;
	void abc() {
		System.out.println("A 클래스 메서드 abc()");
	}
}





public class UseMemberOfOuterClass {
	public static void main(String[] args) {
		
		
	}
}
