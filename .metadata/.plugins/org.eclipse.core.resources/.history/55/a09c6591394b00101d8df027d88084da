package _22_exception.sec01_typeofexception.EX02_UncheckedException;


class A {}
class B extends A {}



public class UncheckedException {
	public static void main(String[] args) {
	
		// UncheckedException = RuntimeException (실행 예외)
		
		// 1. ArtithmeticException
//		System.err.println(3/0);
		
		
		A a = new A();
		B b = (B)a;
		
	}
}
