package control;

public class ForTest2 {
	public static void main(String[] args) {
		
		int sum = 0; // 초기화
		
//		
//		for (int i=1; i<=3; i++) {
//			sum += i;
//		}
		
		
		// 1 ~ 1000까지 3의 배수
		for (int i=1; i<=1000; i++) {
			if (i % 3 == 0) sum +=i;
		}
		
		
	
		System.out.println("종료");
	}
}
